    "@vitejs/plugin-vue": "5.0.4",
    "@vue/compiler-sfc": "3.4.21",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-vue": "9.23.0",

    "@vitejs/plugin-vue": "5.1.4",
    "@vue/compiler-sfc": "3.5.12",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-vue": "9.29.1",
